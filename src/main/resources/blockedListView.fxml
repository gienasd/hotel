<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="420.0" minWidth="600.0" prefHeight="420.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.teamjava.hotel.controllers.BlockedListViewController">
    <children>
        <ImageView fitHeight="420.0" fitWidth="600.0" opacity="0.84" pickOnBounds="true">
            <image>
                <Image url="@images/managment.jpg" />
            </image>
        </ImageView>
        <Label fx:id="labelManagment" layoutX="237.0" layoutY="14.0" text="Lista zablokowanych" textFill="#4d1616">
            <font>
                <Font name="Times New Roman Bold" size="14.0" />
            </font>
            <effect>
                <Reflection />
            </effect>
        </Label>
        <Label fx:id="labelJavaTeam" layoutX="53.0" layoutY="14.0" text="by JavaTeam" textFill="#757575">
            <font>
                <Font name="Times New Roman Bold" size="14.0" />
            </font>
            <effect>
                <Reflection />
            </effect>
        </Label>
        <Button fx:id="buttonLogout" blendMode="COLOR_BURN" layoutX="450.0" layoutY="10.0" mnemonicParsing="false" opacity="0.6" prefHeight="40.0" prefWidth="140.0" text="Wyloguj">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
      <ListView fx:id="listBlocked" layoutX="200.0" layoutY="54.0" prefHeight="300.0" prefWidth="199.0" />
      <Button fx:id="buttonRemove" layoutX="203.0" layoutY="369.0" mnemonicParsing="false" prefWidth="80.0" text="Odblokuj " />
      <Button fx:id="buttonBack" layoutX="27.0" layoutY="370.0" mnemonicParsing="false" text="Wróć" />
      <Button fx:id="buttonProfile" layoutX="320.0" layoutY="369.0" mnemonicParsing="false" prefWidth="80.0" text="Pokaż profil" />
      <Label fx:id="labelLogedUser" layoutX="5.0" layoutY="400.0" prefHeight="20.0" prefWidth="240.0">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
