<AnchorPane minHeight="420.0" minWidth="600.0" prefHeight="420.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.teamjava.hotel.controllers.EditRoomController">
    <children>
        <ImageView fitHeight="420.0" fitWidth="600.0" opacity="0.84" pickOnBounds="true">
            <image>
                <Image url="@images/managment.jpg" />
            </image>
        </ImageView>
        <Label fx:id="labelManagment" layoutX="256.0" layoutY="14.0" text="Edytuj pokój" textFill="#4d0000">
            <font>
                <Font name="Times New Roman Bold" size="14.0" />
            </font>
            <effect>
                <Reflection />
            </effect>
        </Label>
        <Label fx:id="labelJavaTeam" layoutX="53.0" layoutY="14.0" text="by JavaTeam" textFill="#7c7c7c">
            <font>
                <Font name="Times New Roman Bold" size="14.0" />
            </font>
            <effect>
                <Reflection />
            </effect>
        </Label>
        <Button fx:id="buttonLogout" blendMode="COLOR_BURN" layoutX="450.0" layoutY="10.0" mnemonicParsing="false" opacity="0.6" prefHeight="40.0" prefWidth="140.0" text="Wyloguj">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button fx:id="buttonBack" blendMode="COLOR_BURN" layoutX="27.0" layoutY="370.0" mnemonicParsing="false" text="Wróć" />
        <ListView fx:id="listRoom" layoutX="27.0" layoutY="50.0" opacity="0.6" prefHeight="310.0" prefWidth="200.0" />
        <Button fx:id="buttonEdit" layoutX="513.0" layoutY="367.0" mnemonicParsing="false" text="Edytuj" />
        <ScrollBar fx:id="scrollRoom" layoutX="213.0" layoutY="50.0" orientation="VERTICAL" prefHeight="310.0" prefWidth="14.0" />
      <TextField fx:id="textName" layoutX="413.0" layoutY="210.0" promptText="Nowa nazwa" />
      <TextField fx:id="textPrice" layoutX="413.0" layoutY="335.0" promptText="Nowa cena" />
      <ChoiceBox fx:id="choiceCategory" layoutX="413.0" layoutY="255.0" prefWidth="150.0" />
      <ChoiceBox fx:id="choiceCapacity" layoutX="413.0" layoutY="303.0" prefWidth="150.0" />
      <Label layoutX="450.0" layoutY="235.0" text="Kategoria:" />
      <Label layoutX="446.0" layoutY="286.0" text="Ilość miejsc:" />
      <Label fx:id="labelLogedUser" layoutX="5.0" layoutY="400.0" prefHeight="20.0" prefWidth="240.0">
